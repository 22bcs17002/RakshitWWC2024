 // PRINT THE SUM OF N NATURAL NUMBERRS
#include <iostream>
using namespace std;
int main() {
    int a,sum;
    cout<<"enter n ";
    cin>>a;
    for(int i=0;i<=a;i++){
        sum+=i;
    }
    cout << "sum is:"<<sum;
}
//CHECK IF THE NO IS PRIME OR NOT
#include <iostream>
using namespace std;
int main() {
    int num,i;
    cout<<"enter number "<<endl;
    cin>>num;
    if (num <=1){
        cout<<"not prime"<<endl;
    }
        else{
            for(i=2;i*i<=num;i++){
                if(num%i==0){
                cout<<"not prime"<<endl;
                return 0;
                }
            }
                   cout<<"a prime no"; 
                
            }
        
        
    }
